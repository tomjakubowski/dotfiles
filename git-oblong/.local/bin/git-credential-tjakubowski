#!/bin/bash

hostname=$(hostname)
identidad=$0

case $hostname in
  tjakubowskimbp*)
    PATH="/usr/local/opt/git/libexec/git-core:$PATH"
    exec git-credential-osxkeychain "$@"
    ;;
  tjakubowski-sc*)
    exec "/usr/share/doc/git/contrib/credential/libsecret/git-credential-libsecret" "$@"
    ;;
  *) echo "Unknown host for credential helper, add to $identidad"; exit 1 ;;
esac

