#!/bin/bash

hostname=$(hostname)
me=$0

case $hostname in
  tomstation) exec /usr/lib/git-core/git-credential-libsecret "$@"; ;;
  tomtop | tomtop.*) exec /usr/local/opt/git/libexec/git-core/git-credential-osxkeychain "$@"; ;;
  *) echo "Unknown host for credential helper, add to $me"; exit 1 ;;
esac
