[user]
	email = tom@secondspectrum.com
	name = Tom Jakubowski
[push]
	default = simple
[core]
	untrackedCache = true

[transfer]
	fsckObjects = true
[fetch]
	fsckObjects = true
[receive]
	fsckObjects = true
[pull]
	rebase = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[alias]
  co = "checkout"
  p = "pull"
  po = "pull origin"
  pom = "pull origin master"
  P = "push"
  Po = "push origin"
  rb = "rebase"

  update = "push origin"
  fupdate = "push origin --force-with-lease"

[include]
  path = "local.gitconfig"

[url "git@git.2nd.io:"]
  insteadOf = git:

[url "git@git.2nd.io:ai/"]
  insteadOf = ai:

[url "git@git.2nd.io:prism/"]
  insteadOf = prism:

[url "git@git.2nd.io:"]
  insteadOf = https://git.2nd.io/
[rerere]
	enabled = true
